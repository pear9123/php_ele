-- 함수 geojeebook.FN_BUYER_CD 구조 내보내기
DELIMITER //
CREATE DEFINER=`root`@`%` FUNCTION `FN_BUYER_CD`(
	`buyer` INT(10)






) RETURNS varchar(100) CHARSET euckr
BEGIN
   DECLARE rStr varchar(100);SET rStr = null;SELECT
CASE buyer
WHEN 0 THEN 'ETC'
WHEN 1 THEN 'WOORI'
WHEN 2 THEN 'KYOBO'
WHEN 3 THEN 'AUDIEN'
WHEN 4 THEN 'YES24'
WHEN 5 THEN 'ALADIN'
WHEN 6 THEN 'WIZ'
WHEN 7 THEN 'BOOKTOPIA'
WHEN 8 THEN 'BARO'
WHEN 9 THEN 'BOOKCUBE'
WHEN 10 THEN 'YBM'
END INTO rStr;RETURN rStr;END//
DELIMITER ;

-- 함수 geojeebook.FN_BUYER_NM 구조 내보내기
DELIMITER //
CREATE DEFINER=`root`@`%` FUNCTION `FN_BUYER_NM`(
	`buyer` INT(10)







) RETURNS varchar(100) CHARSET euckr
BEGIN
   DECLARE rStr varchar(100);SET rStr = null;SELECT
CASE buyer
WHEN 0 THEN '기타' 
WHEN 1 THEN '우리전자책' 
WHEN 2 THEN '교보전자책' 
WHEN 3 THEN '오디언소리' 
WHEN 4 THEN 'YES24' 
WHEN 5 THEN '알라딘'
WHEN 6 THEN '위즈북'
WHEN 7 THEN '북토피아'
WHEN 8 THEN '바로북'
WHEN 9 THEN '(주)북큐브네트웍스'
WHEN 10 THEN 'YBM'
END INTO rStr;RETURN rStr;END//
DELIMITER ;

